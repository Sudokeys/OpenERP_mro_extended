<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <record id="print_quotation" model="ir.actions.server">
            <field name="name">maintenance print</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">
                for quotation_id in context.get('active_ids'):
                action = self.print_contract(cr, uid, [quotation_id], context=context)
            </field>
        </record>

        <record model="ir.values" id="values_action_print_contract">
            <field name="name">Contrat de maintenance</field>
            <field name="model" eval="'account.analytic.account'" />
            <field name="key" >action</field>
            <field name="key2">client_print_multi</field>
            <field name="value" eval="'ir.actions.server,%d'%print_quotation"/>
        </record>
    </data>
</openerp>

