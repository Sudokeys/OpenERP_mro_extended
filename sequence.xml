<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- Sequences for sale.order -->

        <record id="seq_type_account_analytic_account" model="ir.sequence.type">
            <field name="name">Contract</field>
            <field name="code">account.analytic.account.name</field>
        </record>

        <record id="seq_account_analytic_account" model="ir.sequence">
            <field name="name">Contract name</field>
            <field name="code">account.analytic.account.name</field>
            <field name="prefix">%(year)s%(month)s%(day)s-</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
